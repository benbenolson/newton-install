#%Module1.0#####################################################################
##
## ioapi modulefile
##
## modulefiles/ioapi/3.1. 
##
proc ModulesHelp { } {
        global version
        puts stderr "\tThis module loads ioapi \n"
        puts stderr "\n\tVersion $version\n"
}

module-whatis   "loads ioapi into the working environment."

# for Tcl script use only
set     topdir       /data/apps/ioapi
set version 3.1
set arch Linux2_x86_64

prepend-path  LD_RUN_PATH   $topdir/$version/$arch
prepend-path  INCLUDE  $topdir/$version/ioapi
module load netcdf

