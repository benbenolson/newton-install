#%Module1.0#####################################################################
##
## Emacs modulefile
##
## modulefiles/emacs/23.2. 
##
proc ModulesHelp { } {
        global version
        puts stderr "\tThis module loads Emacs \n"
        puts stderr "\n\tVersion $version\n"
}

module-whatis   "loads Emacs into the working environment."

# for Tcl script use only
set     topdir       /data/apps/emacs
set version 23.2

prepend-path  PATH              $topdir/$version/bin
prepend-path  MANPATH           $topdir/$version/share/man

