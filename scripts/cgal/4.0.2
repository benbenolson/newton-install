#%Module1.0#####################################################################

set     topdir       /data/apps/CGAL
set     version      "4.0.2"
module-whatis        "loads CGAL version $version"

prepend-path  INCLUDE           $topdir/$version/include
prepend-path  LD_LIBRARY_PATH   $topdir/$version/lib
prepend-path  PATH              $topdir/$version/bin
prepend-path  MANPATH           $topdir/$version/share/man

