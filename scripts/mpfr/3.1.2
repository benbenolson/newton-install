#%Module1.0#####################################################################
##
## mpfr modulefile
##
## modulefiles/mpfr. 
##
proc ModulesHelp { } {
        global version
        puts stderr "\tThis module loads mpfr version $version\n"
}

module-whatis   "loads mpfr into the working environment."

# for Tcl script use only
set     topdir       /data/apps/mpfr
set     version      "3.1.2"

prepend-path LD_RUN_PATH $topdir/$version/lib
prepend-path LD_LIBRARY_PATH $topdir/$version/lib
prepend-path MANPATH $topdir/$version/share/info

