#%Module1.0#####################################################################

proc ModulesHelp { } {
        global version
        puts stderr "\tThis module loads the voms-clients version $version \n"
}

module-whatis   "loads the voms-clients into the working environment."

# for Tcl script use only
set     topdir       /data/apps/grid/voms-clients
set     version      "2.0.10-1"

prepend-path MANPATH $topdir/$version/share/man
prepend-path PATH $topdir/$version/bin


