#%Module1.0#####################################################################
##
## SAS modulefile
##
## modulefiles/SAS. 
##
proc ModulesHelp { } {
        global version
        puts stderr "\tThis module loads SAS version 9.2TS2M0 \n"
        puts stderr "\n\tVersion $version\n"
}

module-whatis   "loads SAS into the working environment."

conflict SAS

# for Tcl script use only
set     topdir       /data/apps/SAS
set     version      "9.2TS2M0"

prepend-path  PATH              $topdir/$version/SAS-9.2/SASFoundation/9.2
