#%Module1.0#####################################################################

proc ModulesHelp { } {
        global version
        puts stderr "\tThis module loads cmake\n"
        puts stderr "\n\tVersion $version\n"
}

module-whatis   "loads cmake into the working environment."

# for Tcl script use only
set     topdir       /data/apps/cmake
set     version      "2.8.3"

prepend-path  PATH              $topdir/$version/bin
prepend-path  MANPATH           $topdir/$version/man

