proc ModulesHelp { } {
        global version
        puts stderr "\tThis module loads Mothur version 1.13\n"
        puts stderr "\n\tVersion $version\n"
}

module-whatis   "loads Mothur into the working environment."

prepend-path  PATH              /data/apps/Mothur/1.13-openmpi-intel
prereq openmpi

