#%Module1.0#####################################################################

proc ModulesHelp { } {
        global version
        puts stderr "\tThis module loads COBRA \n"
        puts stderr "\n\tVersion $version\n"
}

module-whatis   "loads COBRA into the working environment."

# for Tcl script use only
set     topdir       /data/apps/cobra
set     version      "2.0.5"

module load sbml

#setenv        MATLAB_USE_USERPATH   1
prepend-path  MATLABPATH            $topdir/$version:$topdir/$version/external/toolboxes/SBMLToolbox-4.1.0/toolbox/
