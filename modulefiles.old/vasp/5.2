#%Module1.0#####################################################################
##
## vasp modulefile
##
## modulefiles/vasp. 
##
proc ModulesHelp { } {
        global version
        puts stderr "\tThis module loads vasp version 5.2 \n"
        puts stderr "\n\tVersion $version\n"
}

module-whatis   "loads vasp into the working environment."

# for Tcl script use only
set     topdir       /data/apps/vasp
set     version      "5.2"

prepend-path  PATH              $topdir/$version/bin
prepend-path INCLUDE		$topdir/$version/lib
prepend-path LD_CONFIG_PATH 	$topdir/$version/lib

prereq intel-compilers/11.1.072

