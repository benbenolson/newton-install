#%Module1.0#####################################################################
##
## udunits modulefile
##
## modulefiles/udunits. 
##
proc ModulesHelp { } {
        global version
        puts stderr "\tThis module loads udunits version 1.12.9 \n"
        puts stderr "\n\tVersion $version\n"
}

module-whatis   "loads udunits into the working environment."

# for Tcl script use only
set     topdir       /data/apps/udunits
set     version      "1.12.9"

prepend-path  PATH              $topdir/$version/bin
prepend-path  MANPATH           $topdir/$version/man
prepend-path  LD_LIBRARY_PATH   $topdir/$version/lib
