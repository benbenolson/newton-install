#%Module1.0#####################################################################
##
## gmp modulefile
##
## modulefiles/gmp/5.0.2 
##
proc ModulesHelp { } {
        global version
        puts stderr "\tThis module loads GMP version $version\n"
        puts stderr "\n\tVersion $version\n"
}

module-whatis   "loads GMP into the working environment."

# for Tcl script use only
set     topdir       /data/apps/gmp
set     version      "5.0.2"

prepend-path LD_RUN_PATH $topdir/$version/lib
prepend-path MANPATH $topdir/$version/share/info

