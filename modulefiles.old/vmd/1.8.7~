#%Module1.0#####################################################################
##
## vmd modulefile
##
## modulefiles/vmd. 
##
proc ModulesHelp { } {
        global version
        puts stderr "\tThis module loads vmd version 1.8.7 \n"
        puts stderr "\n\tVersion $version\n"
}

module-whatis   "loads vmd into the working environment."

# for Tcl script use only
set     topdir       /data/apps/vmd
set     version      "1.8.7"

prepend-path  PATH              $topdir
prepend-path  LD_LIBRARY_PATH   $topdir
