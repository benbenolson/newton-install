#%Module1.0#####################################################################
##
## octave modulefile
##
## modulefiles/octave/3.6.4 
##
proc ModulesHelp { } {
        global version
        puts stderr "\tThis module loads GCC version $version\n"
}

module-whatis   "loads GCC into the working environment."

# for Tcl script use only
set     topdir       /data/apps/octave
set     version      "3.6.4"

prepend-path PATH $topdir/$version/bin
prepend-path LD_RUN_PATH $topdir/$version/lib
prepend-path LD_LIBRARY_PATH $topdir/$version/lib
prepend-path INCLUDE $topdir/$version/include

